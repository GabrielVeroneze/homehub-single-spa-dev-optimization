{
    "$schema": "https://turborepo.com/schema.json",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/"]
        },
        "build:webpack": {
            "dependsOn": ["^build:webpack"],
            "outputs": ["dist/"]
        },
        "prepare": {
            "dependsOn": ["^prepare"]
        },
        "dev": {
            "persistent": true,
            "cache": false
        }
    }
}
